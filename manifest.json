{
  "name": "Track website",
  "version": "1.0",
  "description": "Extension to track a website",
  "permissions": ["webNavigation", "*://*/*","tabs","activeTab","declarativeContent","storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}